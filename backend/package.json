{
	"name": "kyhung-backend",
	"version": "1.0.0",
	"main": "app/app.js",
	"type": "module",
	"scripts": {
		"start": "node app/app.js",
		"dev": "cross-env NODE_ENV=development nodemon -r dotenv/config app/app.js dotenv_config_path=.env.development",
		"prod": "cross-env NODE_ENV=production node -r dotenv/config app/app.js dotenv_config_path=.env.production",
		"test": "cross-env NODE_ENV=test jest --detectOpenHandles",
		"test:watch": "cross-env NODE_ENV=test jest --watch",
		"test:coverage": "cross-env NODE_ENV=test jest --coverage",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"prepare": "husky install"
	},
	"author": "",
	"license": "ISC",
	"description": "Backend API for KyHung application",
	"_moduleAliases": {
		"~": "./app"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.787.0",
		"@google-cloud/storage": "^7.16.0",
		"bcryptjs": "^3.0.2",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"crypto-js": "^4.2.0",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"express-mongo-sanitize": "^2.2.0",
		"express-rate-limit": "^7.5.0",
		"helmet": "^8.1.0",
		"hpp": "^0.2.3",
		"joi": "^17.13.3",
		"jsonwebtoken": "^9.0.2",
		"module-alias": "^2.2.3",
		"mongoose": "^8.12.2",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.2",
		"nodemailer": "^6.10.0",
		"passport": "^0.7.0",
		"passport-google-oauth20": "^2.0.0",
		"passport-jwt": "^4.0.1",
		"passport-local": "^1.0.0",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"winston": "^3.11.0",
		"xss-clean": "^0.1.4"
	},
	"devDependencies": {
		"@eslint/js": "^9.23.0",
		"cross-env": "^7.0.3",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-prettier": "^5.2.3",
		"globals": "^16.0.0",
		"husky": "^9.0.11",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.2",
		"nodemon": "^3.1.9",
		"prettier": "^3.2.5",
		"supertest": "^6.3.4"
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,md}": [
			"prettier --write"
		]
	}
}
